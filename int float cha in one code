#include <stdio.h>

int main()
{
    int a;
    float b;
    char c;
    printf("Enter the value of a\n");
    scanf("%d",&a);
    printf("Enter the value of b\n");
    scanf("%f",&b);
    printf("B is %d\n",(int)b);
    printf("enter a charceter %d\n");
    printf("%c",&c);
    printf("character is %d",(int)c);
    return 0;
    
}
