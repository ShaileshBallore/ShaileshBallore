//mark and age 

#include<stdio.h>
int main()
{
    int age,mark;
    printf("Enter your age\n");
    scanf("%d",&age);
    printf("enter your marks\n");
    scanf("%d",&mark);
    switch(age){
        case 3:
        printf("The age is 3\n");
        switch(mark);
        {
            case 45:
            printf("your marks are 45");
            break;
            printf("your marks are not 45");
        }
        break;
        case 13:
        printf("The age is 13\n");
        break;
        case 23:
        printf("The age is 23 \n");
        break;
        default:
        printf("Age is not 3,13 or 23 \n");
        
    }
    return 0;
}


//do and while use
#include<stdio.h>
int main()
{
    int n,sum =0;
    do{
        printf("enter a number :");
        scanf("%d",&n);
        sum += n;
    
    }
    while (n != 0);
    printf("sum is = %d",sum);
    return 0;
}

//while loop use 
#include<stdio.h>
int main()
{
   int num, index = 1;
   printf("Enter a number\n");
   scanf("%d",&num);
   do{
       printf("%d\n",index + 1);
       index = index + 1;
   }
   while (index < num);

return 0;
}

#include<stdio.h>
int main()
{
  int n, sum =0;
  do {
      printf("Enter a number :");
      scanf("%d",&n);
      sum += n;
     
  }
  while(n != 0);
  printf("sum is = %d",sum);

return 0;
}
//new code
#include<stdio.h>
int main()
{
  int i = 1;
  while(i<=54)
  {
      printf("%d\n",i);
      i = i +1;
  }

return 0;
}

//type number 
#include<stdio.h>
int main()
{
  int num = 10;
  int i;
  for (i=0; i<= num; i++){
      printf("%d\n",i);
  }
  

return 0;
}

