#include <stdio.h>
int main()
{
    int num;
    printf("Enter the number you want the multiplication table of:\n");
    scanf("%d",&num);
    for(int i=1; i<11; i++)
    {
        printf("%d * %d = %d\n",num,i,num*i);
    }
    return 0;
}

//second for same out put

#include <stdio.h>
int main()
{
    int num;
    printf("Enter the number you want the multiplication table of:\n");
    scanf("%d",&num);
    
    {
     printf("multiplication table of %d is as follow s:\n",num);
    printf("%d * 1 = %d\n",num,num*1);
      printf("%d * 2 = %d\n",num,num*2);
     printf("%d * 3 = %d\n",num,num*3);
      printf("%d * 4 = %d\n",num,num*4);
      printf("%d * 5 = %d\n",num,num*5);
    printf("%d * 6 = %d\n",num,num*6);
    printf("%d * 7 = %d\n",num,num*7);
   printf("%d * 8 = %d\n",num,num*8);
    printf("%d * 9 = %d\n",num,num*9);
    printf("%d * 10 = %d\n",num,num*10);
    printf("%d * 11 = %d\n",num,num*11);
    
    }
    return 0;


//OUTPUT 

Enter the number you want the multiplication table of:
8
multiplication table of 8 is as follow s:
8 * 1 = 8
8 * 2 = 16
8 * 3 = 24
8 * 4 = 32
8 * 5 = 40
8 * 6 = 48
8 * 7 = 56
8 * 8 = 64
8 * 9 = 72
8 * 10 = 80
8 * 11 = 88
