#include<stdio.h>
#include<string.h>
 struct student
 {
     int id;
     int marks;
     char fav_char;
     char name[34];
 } harry,ravi,shubham;
 //struct student harry,ravi,shubham;
 
 void print()
 {
     printf("%s",harry.name);
 }
 int main()
 {
     harry.id =1;
     ravi.id =2;
     shubham.id = 3;
     harry.marks = 456;
     ravi.marks = 585;
     shubham.marks = 876;
     harry.fav_char = 'H';
     ravi.fav_char = 'P';
     shubham.fav_char = 'S';
     
     strcpy(harry.name,"Harry Potter Student of the year");
     strcpy(shubham.name,"Shubham Kumar");
     printf("Harry got %d marks\n",harry.marks);
     printf("Harry's name is:%s\n",harry.marks);
     printf("shubham got %d marks\n",shubham.marks);
      printf("shubham's name is:%s\n",shubham.marks);
     printf("Ravi got %d marks\n",harry.marks);
      printf("Ravi's name is:%s\n",ravi.marks);
     
     print();
     return 0;
 }

//OUTPUT OF THE CODE
Harry got 456 marks
Segmentation fault


